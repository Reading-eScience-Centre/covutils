{
  "name": "covutils",
  "version": "0.4.2",
  "license": "BSD-3-Clause",
  "description": "Utilities for creating, transforming, and handling Coverage Data objects.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Reading-eScience-Centre/covutils.git"
  },
  "bugs": {
    "url": "https://github.com/Reading-eScience-Centre/covutils/issues"
  },
  "author": {
    "name": "Maik Riechert"
  },
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "src/",
    "lib/",
    "covutils.*",
    "covutils-lite.*"
  ],
  "dependencies": {
    "ndarray": "^1.0.18",
    "point-in-big-polygon": "^2.0.0",
    "topojson": "^1.6.26"
  },
  "jspm": {
    "registry": "npm",
    "format": "esm",
    "main": "src/index.js"
  },
  "scripts": {
    "esdoc": "esdoc -c esdoc.json",
    "docs": "documentation build src/index.js -f html -o docs/",
    "transpile": "babel -d lib/ src/",
    "test": "standard && npm run testnode && npm run testkarma",
    "testnode": "mocha --recursive --compilers js:babel-register",
    "testkarma": "karma start --single-run",
    "karma": "karma start",
    "saucelabs": "karma start karma.conf-ci.js",
    "browserdist": "npm run browserdistfull && npm run browserdistlite",
    "browserdistlite": "rollup -c rollup.config.lite-src.js && rollup -c rollup.config.lite-min.js",
    "browserdistfull": "rollup -c rollup.config.src.js && rollup -c rollup.config.min.js",
    "build": "npm run transpile && npm run browserdist",
    "prepublish": "npm run build"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "karma.conf.js",
      "karma.conf-ci.js",
      "test/"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-register": "^6.9.0",
    "babelify": "^7.2.0",
    "browserify": "^11.0.1",
    "core-js": "^2.0.2",
    "covjson-reader": "^0.12.1",
    "esdoc": "^0.4.1",
    "esdoc-importpath-plugin": "0.0.1",
    "documentation": "^4.0.0-beta5",
    "finalhandler": "^0.4.1",
    "karma": "^0.13.9",
    "karma-browserify": "^4.4.2",
    "karma-chrome-launcher": "^0.2.0",
    "karma-mocha": "^0.2.0",
    "karma-sauce-launcher": "^0.2.14",
    "mocha": "^2.5.3",
    "rollup": "^0.30.0",
    "rollup-plugin-babel": "^2.5.1",
    "rollup-plugin-commonjs": "^3.1.0",
    "rollup-plugin-node-resolve": "^1.7.1",
    "rollup-plugin-uglify": "^1.0.1",
    "serve-static": "^1.10.0",
    "standard": "^5.1.0",
    "uglify-js": "^2.4.24",
    "xndarray": "^0.3.0"
  }
}
